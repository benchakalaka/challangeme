<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/ll"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@drawable/bg" >

	<LinearLayout
		android:id="@+id/ll2"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:layout_weight="3"
		android:orientation="vertical"
		tools:context="${packageName}.${activityClass}" >

		<RelativeLayout
			android:id="@+id/rl1"
			android:layout_width="fill_parent"
			android:layout_height="70dp"
			android:layout_margin="5dp"
			android:orientation="vertical" >

			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerInParent="true"
				android:background="@drawable/circle"
				android:gravity="center"
				android:lines="1"
				android:maxLines="1"
				android:singleLine="true"
				android:text="1"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:textColor="@android:color/white"
				android:textStyle="bold" />
		</RelativeLayout>

		<RelativeLayout
			android:id="@+id/rl2"
			android:layout_width="fill_parent"
			android:layout_height="70dp"
			android:layout_margin="5dp"
			android:orientation="vertical" >

			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerInParent="true"
				android:background="@drawable/circle"
				android:gravity="center"
				android:lines="1"
				android:maxLines="1"
				android:singleLine="true"
				android:text="2"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:textColor="@android:color/white"
				android:textStyle="bold" />
		</RelativeLayout>

		<RelativeLayout
			android:id="@+id/rl3"
			android:layout_width="fill_parent"
			android:layout_height="70dp"
			android:layout_margin="5dp"
			android:orientation="vertical" >

			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerInParent="true"
				android:background="@drawable/circle"
				android:gravity="center"
				android:lines="1"
				android:maxLines="1"
				android:singleLine="true"
				android:text="3"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:textColor="@android:color/white"
				android:textStyle="bold" />
		</RelativeLayout>

		<RelativeLayout
			android:id="@+id/rl4"
			android:layout_width="fill_parent"
			android:layout_height="70dp"
			android:layout_margin="5dp"
			android:orientation="vertical" >

			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerInParent="true"
				android:background="@drawable/circle"
				android:gravity="center"
				android:singleLine="true"
				android:text="4"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:textColor="@android:color/white"
				android:textStyle="bold" />
		</RelativeLayout>

		<RelativeLayout
			android:id="@+id/rl5"
			android:layout_width="fill_parent"
			android:layout_height="70dp"
			android:layout_margin="5dp"
			android:orientation="vertical" >

			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerInParent="true"
				android:background="@drawable/circle"
				android:gravity="center"
				android:singleLine="true"
				android:text="5"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:textColor="@android:color/white"
				android:textStyle="bold" />
		</RelativeLayout>
	</LinearLayout>

	<LinearLayout
		android:id="@+id/llRoot"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:layout_weight="1"
		android:orientation="vertical"
		tools:context="${packageName}.${activityClass}" >

		<RelativeLayout
			android:id="@+id/rlSimple"
			android:layout_width="fill_parent"
			android:layout_height="70dp"
			android:layout_margin="5dp"
			android:background="@drawable/transparent_inside_and_white_border"
			android:orientation="vertical" >

			<ImageView
				android:id="@+id/image"
				android:layout_width="60dp"
				android:layout_height="60dp"
				android:layout_alignParentLeft="true"
				android:layout_centerVertical="true"
				android:layout_marginLeft="5dp"
				android:background="@drawable/simple" />

			<TextView
				android:id="@+id/text1"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:layout_centerVertical="true"
				android:layout_gravity="center_vertical"
				android:layout_toLeftOf="@+id/ivSimpleUp"
				android:layout_toRightOf="@+id/image"
				android:gravity="center_horizontal"
				android:lines="1"
				android:maxLines="1"
				android:singleLine="true"
				android:text="@string/simple_note_type"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:textColor="@android:color/white"
				android:textStyle="bold" />

			<ImageView
				android:id="@+id/ivSimpleUp"
				android:layout_width="40dp"
				android:layout_height="40dp"
				android:layout_centerVertical="true"
				android:layout_margin="5dp"
				android:layout_toLeftOf="@+id/ivSimpleDown"
				android:background="@drawable/arrowup"
				android:padding="5dp"
				android:tag="up" />

			<ImageView
				android:id="@+id/ivSimpleDown"
				android:layout_width="40dp"
				android:layout_height="40dp"
				android:layout_alignParentRight="true"
				android:layout_centerVertical="true"
				android:layout_margin="5dp"
				android:background="@drawable/arrowdown"
				android:padding="5dp"
				android:tag="down" />
		</RelativeLayout>

		<RelativeLayout
			android:id="@+id/rlEvent"
			android:layout_width="fill_parent"
			android:layout_height="70dp"
			android:layout_margin="5dp"
			android:background="@drawable/transparent_inside_and_white_border"
			android:orientation="vertical" >

			<ImageView
				android:id="@+id/ivEvent"
				android:layout_width="60dp"
				android:layout_height="60dp"
				android:layout_alignParentLeft="true"
				android:layout_centerVertical="true"
				android:layout_marginLeft="5dp"
				android:background="@drawable/event" />

			<TextView
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:layout_centerVertical="true"
				android:layout_gravity="center_vertical"
				android:layout_toLeftOf="@+id/ivEventUp"
				android:layout_toRightOf="@+id/ivEvent"
				android:gravity="center_horizontal"
				android:lines="1"
				android:maxLines="1"
				android:singleLine="true"
				android:text="@string/event_note_type"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:textColor="@android:color/white"
				android:textStyle="bold" />

			<ImageView
				android:id="@+id/ivEventUp"
				android:layout_width="40dp"
				android:layout_height="40dp"
				android:layout_centerVertical="true"
				android:layout_margin="5dp"
				android:layout_toLeftOf="@+id/ivEventDown"
				android:background="@drawable/arrowup"
				android:padding="5dp"
				android:tag="up" />

			<ImageView
				android:id="@+id/ivEventDown"
				android:layout_width="40dp"
				android:layout_height="40dp"
				android:layout_alignParentRight="true"
				android:layout_centerVertical="true"
				android:layout_margin="5dp"
				android:background="@drawable/arrowdown"
				android:padding="5dp"
				android:tag="down" />
		</RelativeLayout>

		<RelativeLayout
			android:id="@+id/rlUrgent"
			android:layout_width="fill_parent"
			android:layout_height="70dp"
			android:layout_margin="5dp"
			android:background="@drawable/transparent_inside_and_white_border"
			android:orientation="vertical" >

			<ImageView
				android:id="@+id/ivUpss"
				android:layout_width="60dp"
				android:layout_height="60dp"
				android:layout_alignParentLeft="true"
				android:layout_centerVertical="true"
				android:layout_marginLeft="5dp"
				android:background="@drawable/urgent" />

			<TextView
				android:id="@+id/text3"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:layout_centerVertical="true"
				android:layout_gravity="center_vertical"
				android:layout_toLeftOf="@+id/ivUrgentUp"
				android:layout_toRightOf="@+id/ivUpss"
				android:gravity="center_horizontal"
				android:lines="1"
				android:maxLines="1"
				android:singleLine="true"
				android:text="@string/urgent_note_type"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:textColor="@android:color/white"
				android:textStyle="bold" />

			<ImageView
				android:id="@+id/ivUrgentUp"
				android:layout_width="40dp"
				android:layout_height="40dp"
				android:layout_centerVertical="true"
				android:layout_margin="5dp"
				android:layout_toLeftOf="@+id/ivUrgentDown"
				android:background="@drawable/arrowup"
				android:padding="5dp"
				android:tag="up" />

			<ImageView
				android:id="@+id/ivUrgentDown"
				android:layout_width="40dp"
				android:layout_height="40dp"
				android:layout_alignParentRight="true"
				android:layout_centerVertical="true"
				android:layout_margin="5dp"
				android:background="@drawable/arrowdown"
				android:padding="5dp"
				android:tag="down" />
		</RelativeLayout>

		<RelativeLayout
			android:id="@+id/rlReminder"
			android:layout_width="fill_parent"
			android:layout_height="70dp"
			android:layout_margin="5dp"
			android:background="@drawable/transparent_inside_and_white_border"
			android:orientation="vertical" >

			<ImageView
				android:id="@+id/ivReminder"
				android:layout_width="60dp"
				android:layout_height="60dp"
				android:layout_alignParentLeft="true"
				android:layout_centerVertical="true"
				android:layout_marginLeft="5dp"
				android:background="@drawable/reminder" />

			<TextView
				android:id="@+id/text4"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:layout_centerVertical="true"
				android:layout_gravity="center_vertical"
				android:layout_toLeftOf="@+id/ivReminderUp"
				android:layout_toRightOf="@+id/ivReminder"
				android:gravity="center_horizontal"
				android:lines="1"
				android:maxLines="1"
				android:singleLine="true"
				android:text="@string/reminder_note_type"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:textColor="@android:color/white"
				android:textStyle="bold" />

			<ImageView
				android:id="@+id/ivReminderUp"
				android:layout_width="40dp"
				android:layout_height="40dp"
				android:layout_centerVertical="true"
				android:layout_margin="5dp"
				android:layout_toLeftOf="@+id/ivReminderDown"
				android:background="@drawable/arrowup"
				android:padding="5dp"
				android:tag="up" />

			<ImageView
				android:id="@+id/ivReminderDown"
				android:layout_width="40dp"
				android:layout_height="40dp"
				android:layout_alignParentRight="true"
				android:layout_centerVertical="true"
				android:layout_margin="5dp"
				android:background="@drawable/arrowdown"
				android:padding="5dp"
				android:tag="down" />
		</RelativeLayout>

		<RelativeLayout
			android:id="@+id/rlWork"
			android:layout_width="fill_parent"
			android:layout_height="70dp"
			android:layout_margin="5dp"
			android:background="@drawable/transparent_inside_and_white_border"
			android:orientation="vertical" >

			<ImageView
				android:id="@+id/imageNotifications"
				android:layout_width="60dp"
				android:layout_height="60dp"
				android:layout_alignParentLeft="true"
				android:layout_centerVertical="true"
				android:layout_marginLeft="5dp"
				android:background="@drawable/work" />

			<TextView
				android:id="@+id/textNotifications"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:layout_centerVertical="true"
				android:layout_gravity="center_vertical"
				android:layout_toLeftOf="@+id/ivWorkUp"
				android:layout_toRightOf="@+id/imageNotifications"
				android:gravity="center_horizontal"
				android:lines="1"
				android:maxLines="1"
				android:singleLine="true"
				android:text="@string/work_note_type"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:textColor="@android:color/white"
				android:textStyle="bold" />

			<ImageView
				android:id="@+id/ivWorkUp"
				android:layout_width="40dp"
				android:layout_height="40dp"
				android:layout_centerVertical="true"
				android:layout_margin="5dp"
				android:layout_toLeftOf="@+id/ivWorkDown"
				android:background="@drawable/arrowup"
				android:padding="5dp"
				android:tag="up" />

			<ImageView
				android:id="@+id/ivWorkDown"
				android:layout_width="40dp"
				android:layout_height="40dp"
				android:layout_alignParentRight="true"
				android:layout_centerVertical="true"
				android:layout_margin="5dp"
				android:background="@drawable/arrowdown"
				android:padding="5dp"
				android:tag="down" />
		</RelativeLayout>
	</LinearLayout>

</LinearLayout>