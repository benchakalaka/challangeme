<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/slidemenu_root_scroll_view"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@drawable/bg_slidemenu" >

	<LinearLayout
		android:id="@+id/rootLL"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical" >

		<RelativeLayout
			android:id="@+id/notes"
			android:layout_width="match_parent"
			android:layout_height="wrap_content" >

			<ImageView
				android:id="@+id/imageViewLeftArrowMenu22"
				android:layout_width="50dip"
				android:layout_height="50dip"
				android:layout_margin="5dp"
				android:contentDescription="@null"
				android:padding="5dp"
				android:src="@drawable/book" />

			<TextView
				android:id="@+id/textView122"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_alignParentTop="true"
				android:layout_toRightOf="@+id/imageViewLeftArrowMenu22"
				android:padding="20dp"
				android:text="Notes"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:textColor="@android:color/white" />

			<TextView
				android:id="@+id/twAmoutFinished"
				android:layout_width="30dp"
				android:layout_height="30dp"
				android:layout_alignParentRight="true"
				android:background="@drawable/circle_green"
				android:gravity="center"
				android:padding="2dp"
				android:text="2"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:textColor="@android:color/white" />

			<TextView
				android:id="@+id/twAmoutNotes"
				android:layout_width="30dp"
				android:layout_height="30dp"
				android:background="@drawable/circle_red"
				android:clipChildren="false"
				android:gravity="center"
				android:padding="2dp"
				android:text="2"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:textColor="@android:color/white" />

			<View
				android:layout_width="fill_parent"
				android:layout_height="1dp"
				android:layout_alignParentBottom="true"
				android:layout_marginLeft="10dp"
				android:layout_marginRight="10dp"
				android:background="@color/slide_menu_divider_color" />
		</RelativeLayout>

		<RelativeLayout
			android:id="@+id/filter"
			android:layout_width="match_parent"
			android:layout_height="wrap_content" >

			<ImageView
				android:id="@+id/imageViaewLefatArraowMenu22"
				android:layout_width="50dip"
				android:layout_height="50dip"
				android:layout_margin="5dp"
				android:contentDescription="@null"
				android:padding="5dp"
				android:src="@drawable/filter" />

			<TextView
				android:id="@+id/textView12q2"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignParentRight="true"
				android:layout_alignParentTop="true"
				android:layout_toRightOf="@+id/imageViaewLefatArraowMenu22"
				android:padding="20dp"
				android:text="Filter"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:textColor="@android:color/white" />

			<View
				android:layout_width="fill_parent"
				android:layout_height="1dp"
				android:layout_alignParentBottom="true"
				android:layout_marginLeft="10dp"
				android:layout_marginRight="10dp"
				android:background="@color/slide_menu_divider_color" />
		</RelativeLayout>

		<RelativeLayout
			android:id="@+id/settings"
			android:layout_width="match_parent"
			android:layout_height="wrap_content" >

			<ImageView
				android:id="@+id/imageViewLeftArrowMeasddasnu"
				android:layout_width="50dip"
				android:layout_height="50dip"
				android:layout_margin="5dp"
				android:contentDescription="@null"
				android:padding="5dp"
				android:src="@drawable/settings" />

			<TextView
				android:id="@+id/textVieasdasw1"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignParentRight="true"
				android:layout_alignParentTop="true"
				android:layout_toRightOf="@+id/imageViewLeftArrowMeasddasnu"
				android:padding="20dp"
				android:text="Settings"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:textColor="@android:color/white" />

			<View
				android:layout_width="fill_parent"
				android:layout_height="1dp"
				android:layout_alignParentBottom="true"
				android:layout_marginLeft="10dp"
				android:layout_marginRight="10dp"
				android:background="@color/slide_menu_divider_color" />
		</RelativeLayout>

		<RelativeLayout
			android:id="@+id/logout"
			android:layout_width="match_parent"
			android:layout_height="wrap_content" >

			<ImageView
				android:id="@+id/imageVaaaiewLeftArrowMenu"
				android:layout_width="50dip"
				android:layout_height="50dip"
				android:layout_margin="5dp"
				android:contentDescription="@null"
				android:padding="5dp"
				android:src="@drawable/logout" />

			<TextView
				android:id="@+id/teaxstViaew1"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignParentRight="true"
				android:layout_alignParentTop="true"
				android:layout_toRightOf="@+id/imageVaaaiewLeftArrowMenu"
				android:padding="20dp"
				android:text="Logout"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:textColor="@android:color/white" />

			<View
				android:layout_width="fill_parent"
				android:layout_height="1dp"
				android:layout_alignParentBottom="true"
				android:layout_marginLeft="10dp"
				android:layout_marginRight="10dp"
				android:background="@color/slide_menu_divider_color" />
		</RelativeLayout>

		<RelativeLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content" >

			<ImageView
				android:id="@+id/ivHelp"
				android:layout_width="40dip"
				android:layout_height="40dip"
				android:layout_toRightOf="@+id/tvHelp"
				android:contentDescription="@null"
				android:padding="5dp"
				android:src="@drawable/urgent" />

			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignParentBottom="true"
				android:layout_alignParentLeft="true"
				android:layout_alignParentRight="true"
				android:layout_below="@+id/ivHelp"
				android:padding="20dp"
				android:text="@string/help"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:textColor="@color/slide_menu_divider_color" />

			<TextView
				android:id="@+id/tvHelp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignBottom="@+id/ivHelp"
				android:layout_alignParentTop="true"
				android:layout_centerHorizontal="true"
				android:gravity="center"
				android:text="Help"
				android:textAppearance="?android:attr/textAppearanceLarge"
				android:textColor="@color/selected_color" />
		</RelativeLayout>
	</LinearLayout>

</ScrollView>